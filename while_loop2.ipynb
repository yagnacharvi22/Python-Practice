      "source": [
        "# Write a program to display the number in reverse order\n",
        "# input: 6324  ---\n",
        "# output: 4236\n",
        "\n",
        "num=6824\n",
        "rev=0\n",
        "while num!=0:            # 6!=0\n",
        "    rem=num%10               # 6%10   =6\n",
        "    rev=rev*10+rem       #4236\n",
        "    num=num//10              # 6//10  6\n",
        "print(\"reverse number: \",rev)\n"
      ],
